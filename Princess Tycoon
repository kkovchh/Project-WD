local AutoFarm = Window:NewTab("AutoFarm")

local FarmSection = AutoFarm:NewSection("Auto Farm")

FarmSection:NewButton("Block Farming", "This Farm is used for money blocks", function()	-- obtener la carpeta donde se encuentran las partes
	local carpeta = game.Workspace.CrateParent

	-- iterar sobre cada parte dentro de la carpeta
	for _,parte in pairs(carpeta:GetChildren()) do
		-- teletransportar la parte al jugador local
		parte.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
	end

end)





