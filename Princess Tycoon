	end
end)

SectionLaseres:NewButton("Remove lasers from Jade", "destroy", function()
	local carpeta =	game.Workspace.Tycoons.Tycoons.Jade.PurchasedObjects.Door.Lasers

	-- Obtener la lista de objetos dentro de la carpeta
	local objetos = carpeta:GetChildren()

	for i, objeto in ipairs(objetos) do
		objeto:Destroy()
	end
end)

