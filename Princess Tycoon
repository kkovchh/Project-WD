local destrucionplus = Window:NewTab("Destrucion+")

local sectiondestrucionplus = destrucionplus:NewSection("Destrucion+")

sectiondestrucionplus:NewLabel("Workspace")
-- Función que se ejecutará por cada instancia en el workspace
local function miFuncion1(instancia)
	sectiondestrucionplus:NewButton("Delete "..instancia.Name, "Delete the instance", function()
		instancia:Destroy()
	end)
end

-- Obtener todas las instancias dentro del workspace
local children = game.Workspace:GetChildren()

-- Loop para ejecutar la función para cada instancia
for i, instancia in ipairs(children) do
	miFuncion1(instancia)
end

-- Función que se ejecutará por cada instancia en el workspace
local function miFuncion2(instancia)
	sectiondestrucionplus:NewButton("Delete "..instancia.Name, "Delete the instance", function()
		instancia:Destroy()
	end)
end

-- Obtener todas las instancias dentro del workspace
local children = game.Workspace.Tycoons.Tycoons:GetChildren()

-- Loop para ejecutar la función para cada instancia
for i, instancia in ipairs(children) do
	miFuncion2(instancia)
end

-- Función que se ejecutará por cada instancia en el workspace
local function miFuncion3(instancia)
	sectiondestrucionplus:NewButton("Delete "..instancia.Name, "Delete the instance", function()
		instancia:Destroy()
	end)
end

-- Obtener todas las instancias dentro del workspace
local children = game.Workspace.Map:GetChildren()

-- Loop para ejecutar la función para cada instancia
for i, instancia in ipairs(children) do
	miFuncion3(instancia)
end

-- Función que se ejecutará por cada instancia en el workspace
local function miFuncion4(instancia)
	sectiondestrucionplus:NewButton("Delete "..instancia.Name, "Delete the instance", function()
		instancia:Destroy()
	end)
end

-- Obtener todas las instancias dentro del workspace
local children = game.Players.LocalPlayer.PlayerGui:GetChildren()

-- Loop para ejecutar la función para cada instancia
for i, instancia in ipairs(children) do
	miFuncion4(instancia)
end


