local laseres = Window:NewTab("Destruction")

local SectionLaseres = laseres:NewSection("Destruction")


SectionLaseres:NewLabel("THIS WILL WORK FOR THE TYCOONS THAT HAVE LASERS")

SectionLaseres:NewButton("Remove lasers from moon", "destroy", function()
	local carpeta =	game.Workspace.Tycoons.Tycoons.Moon.PurchasedObjects.Door.Lasers
	
	-- Obtener la lista de objetos dentro de la carpeta
	local objetos = carpeta:GetChildren()
	
	for i, objeto in ipairs(objetos) do
		objeto:Destroy()
	end
end)


